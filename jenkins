pipeline {
    agent any
    stages {
        stage('build') {
            steps {
                sh 'whoami'
            }
        }
 stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/kmmeena/temp.git' 
            }
        }

        stage('Deploy') {

            steps {
                // Your deployment commands here
                // Example using a simple copy command (replace with your actual deployment logic)
                //sh 'cp -r /var/lib/jenkins/workspace/firstpipeline /var/www/html' 
		sh 'git clone https://github.com/kmmeena/temp.git /var/www/html/' 
			  
            }
    }

}
}
